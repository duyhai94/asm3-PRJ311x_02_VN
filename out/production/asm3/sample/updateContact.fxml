<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.DatePicker?>
<AnchorPane
        prefWidth="300" prefHeight="353.0"
        xmlns:fx="http://javafx.com/fxml"
        fx:controller="sample.UpdateContactController">
    <children>
        <Label layoutX="24.0" layoutY="23.0" prefWidth="194.0" prefHeight="22.0" text="Update a Contact">
            <font>
                <Font name="System Bold " size="17.0"/>
            </font>
        </Label>

        <Label layoutX="24.0" layoutY="63.0" text="First Name"/>
        <TextField fx:id="firstName" layoutX="105.0" layoutY="58.0" text="An"/>
        <Label layoutX="24.0" layoutY="99.0" text="Last Name"/>
        <Label layoutX="24.0" layoutY="133.0" text="Phone"/>
        <Label layoutX="24.0" layoutY="166.0" text="Email"/>
        <Label layoutX="24.0" layoutY="207.0" text="Birth date"/>
        <Label layoutX="24.0" layoutY="254.0" text="Group"/>

        <TextField fx:id="lastName" layoutX="105.0" layoutY="94.0" text="Ha"/>
        <TextField fx:id="phone" layoutX="105.0" layoutY="128.0" text="123456789"/>
        <TextField fx:id="Email" layoutX="105.0" layoutY="170.0" text="Ha123@gmail.com"/>
        <DatePicker fx:id="dob" layoutX="105.0" layoutY="221.0"/>
        <ComboBox fx:id="cbGroup" layoutX="105.0" layoutY="249.0" prefWidth="150.0"/>
        <Button fx:id="btnAdd" layoutX="25.0" layoutY="308.0" mnemonicParsing="false" text="Save" />
        <Button fx:id="btnClose" layoutX="99.0" layoutY="308.0" mnemonicParsing="false" text="Close" />



    </children>
</AnchorPane>